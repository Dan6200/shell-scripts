vimdiff ~/.config/i3/config ~/dotfiles/i3-config
vimdiff ~/.tmux.conf ~/dotfiles/.tmux.conf
vimdiff ~/.zshrc ~/dotfiles/.zshrc
vimdiff ~/.vimrc ~/dotfiles/.vimrc
cp ~/.config/i3/config ~/dotfiles/i3-config
cp ~/.tmux.conf ~/dotfiles/.tmux.conf
cp ~/.zshrc ~/dotfiles/.zshrc
cp ~/.vimrc ~/dotfiles/.vimrc
git -C ~/dotfiles commit -a
git -C ~/dotfiles push
